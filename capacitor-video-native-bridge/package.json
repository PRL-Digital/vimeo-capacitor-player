{
  "name": "capacitor-video-native-bridge",
  "version": "0.1.0",
  "description": "Capacitor plugin that bridges native iOS video player with JavaScript",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "ios/Package.swift",
    "VimeoCapacitorVideoNativeBridge.podspec"
  ],
  "author": "Vimeo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PRL-Digital/vimeo-capacitor-player"
  },
  "bugs": {
    "url": "https://github.com/PRL-Digital/vimeo-capacitor-player/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "video",
    "ios",
    "vimeo",
    "bridge"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "build": "npm run clean && mkdir -p dist && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/docgen": "^0.2.0",
    "@capacitor/ios": "^6.0.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^4.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-java": "^2.5.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "typescript": "~5.3.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^6.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
